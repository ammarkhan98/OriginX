# VPN Server Configuration

# Option 1: Use local JSON configuration file
# Set the path to your servers configuration file
VPN_SERVERS_CONFIG_PATH=./src/config/servers.json

# Option 2: Use remote API for dynamic server updates
# Set the URL to your remote servers API endpoint
# Example API response format:
# {
#   "servers": [
#     {
#       "id": "us-ny-1",
#       "name": "United States - New York",
#       "countryCode": "US",
#       "city": "New York",
#       "ip": "45.33.32.156",
#       "protocol": "UDP",
#       "load": 35,
#       "ping": 12
#     }
#   ]
# }
# VPN_SERVERS_API_URL=https://api.your-vpn-service.com/servers

# Backend Configuration
BACKEND_PORT=9999
BACKEND_HOST=localhost

# Frontend Configuration
REACT_APP_API_URL=http://localhost:9999
REACT_APP_ENV=development

# VPN Settings
VPN_DNS_PRIMARY=8.8.8.8
VPN_DNS_SECONDARY=8.8.4.4
VPN_PROTOCOL=UDP
VPN_ENCRYPTION_LEVEL=high
